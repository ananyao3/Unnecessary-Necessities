#include <stdio.h>
#include <assert.h>

int fibonnaci_num(const int seq_num, 
                  const int start_val_1, 
                  const int start_val_2) {
  
  printf("The Fibonnaci Sequence at %d with starting ", seq_num);
  printf("values %d and %d is: ", start_val_1, start_val_2);

  int prev = start_val_1;
  int current = start_val_2;
  int holder = 0;

  for (int count = 1; count < seq_num; ++count) {
    if (count == 1) {
      printf("%d, ", prev);
    } else if (count == 2) {
      printf("%d, ", current);
    } else {
      holder = prev + current;
      printf("%d, ", holder);
      prev = current;
      current = holder;
    }
  }

  holder = prev + current;
  printf("%d\n", holder);
  prev = current;
  current = holder;

  return 1;
}



int main(void) {
  assert(fibonnaci_num(6, 0, 1) == 1);
}
